[
  {
    "name": "itzg-bedrock",
    "image": "itzg/minecraft-bedrock-server:latest",
    "essential": true,
    "logConfiguration": {
      "logDriver": "awslogs",
      "secretOptions": null,
      "options": {
        "awslogs-group": "/ecs/bedrock-fargate",
        "awslogs-region": "us-west-2",
        "awslogs-stream-prefix": "ecs"
      }
    },
    "portMappings": [
      {
        "containerPort": 19132,
        "protocol": "udp",
        "hostPort": 19132
      }
    ],
    "environment": [
      {
        "name": "EULA",
        "value": "TRUE"
      },
      {
        "name": "ALLOW_CHEATS",
        "value": "true"
      },
      {
        "name": "GID",
        "value": "1000"
      },
      {
        "name": "UID",
        "value": "1000"
      }
    ],
    "mountPoints": [
      {
        "readOnly": null,
        "containerPath": "/data",
        "sourceVolume": "bedrock-efs"
      }
    ]
  }
]
